}

.editPageNav {
  height: 7%;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 3%;
  font-size: larger;
  font-weight:600;
  color:#fff;
  background-color: #409eff;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;

<!--pages/editBrandPage/editBrandPage.wxml-->
<view>
  <!-- 自定义导航栏 -->
  <view class="nav-bar" style="height:{{navBarHeight}}px">
    <view class="title" style="height:{{menuHeight}}px;bottom:{{menuBottom}}px;line-height:{{menuHeight}}px;display: flex;">
      <van-icon name="arrow-left" size="30px" style="margin-top: 80rpx;margin-left: 10rpx;" bindtap="backBtn" />
      <view style="margin-left: 240rpx;margin-top: 45rpx;font-size:35rpx">编辑品牌</view>
    </view>
  </view>
  <view class="uploadParts">
    <van-uploader file-list="{{ fileList }}" bind:after-read="afterRead" />
  </view>
</view>
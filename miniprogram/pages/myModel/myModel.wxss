}

.wrapper{
  display: flex;
  align-items: flex-end;
  justify-content: center;
  height: 100vh;
}

.block {
  width: 100%;
  height: 80%;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;

<!--pages/detailsPage/detailsPage.wxml-->
<view>
  <view class="nav-bar" style="height:{{navBarHeight}}px;background:#F6F9FE;position: fixed;width: 100%;top: 0px;z-index: 999;">
    <view class="title" style="height:{{menuHeight}}px;bottom:{{menuBottom}}px;line-height:{{menuHeight}}px;display: flex;justify-content:space-between">
      <van-icon name="arrow-left" size="30px" style="margin-top: 80rpx;" bindtap="backBtn" />
      <view style="margin-top: 45rpx; font-size:35rpx">霸王龙威尔逊</view>
      <view style="width: 60rpx;height: 10rpx;"></view>
    </view>
  </view>
  <view class="mainparts" style="margin-top:{{navBarHeight}}px">
    <view class="mainNav">
      <view class="navNav">
        <view>
          <van-image width="100" height="100" fit="cover" src="https://img.yzcdn.cn/vant/cat.jpeg" class="atver"></van-image>
        </view>
        <view>
          <view style="color: #2E3E63;font-weight:bold;font-size: 40rpx;">霸王龙威尔逊</view>
          <view style="font-size: 28rpx;line-height: 60rpx;">品牌:PNSO恐龙大王</view>
          <view style="margin-top: 30rpx;">
            <van-button color="#9CA5B6" style="margin-right: 40rpx;" size="small" round>
              <van-icon name="like-o" style="margin-right: 15rpx;" />
              想入
            </van-button>
            <van-button color="#9CA5B6" size="small" round>
              <van-icon name="like" style="margin-right: 15rpx;" />
              已入
            </van-button>
          </view>
        </view>
      </view>
      <view class="modelScore">
        <view class="leftParts">
          <view style="font-size: 30rpx;color: #515F7D;padding-left: 20rpx;padding-top: 20rpx;">
            模型评分
          </view>
          <view style="width: 50%;margin-left: 20rpx;">
            <view style="font-size: 25rpx;margin-right: 10rpx;">9-10</view>
            <view style="margin-left: 30px;margin-top: -20rpx;">
              <van-progress color="#FE8D19" percentage="75" track-color="#FFEEDA" stroke-width="4px"></van-progress>

            </view>
            <view>
            </view>
          </view>
          <view style="width: 50%;margin-left: 20rpx;margin-top: 10rpx;">
            <view style="font-size: 25rpx;margin-right: 10rpx;">7-8</view>
            <view style="margin-left: 30px;margin-top: -20rpx;">
              <van-progress color="#FE8D19" percentage="60" track-color="#FFEEDA" stroke-width="4px"></van-progress>

            </view>
            <view>
            </view>
          </view>
          <view style="width: 50%;margin-left: 20rpx;margin-top: 10rpx">
            <view style="font-size: 25rpx;margin-right: 10rpx;">5-6</view>
            <view style="margin-left: 30px;margin-top: -20rpx;">
              <van-progress color="#FE8D19" percentage="45" track-color="#FFEEDA" stroke-width="4px"></van-progress>

            </view>
            <view>
            </view>
          </view>
          <view style="width: 50%;margin-left: 20rpx;margin-top: 10rpx">
            <view style="font-size: 25rpx;margin-right: 10rpx;">3-4</view>
            <view style="margin-left: 30px;margin-top: -20rpx;">
              <van-progress color="#FE8D19" percentage="30" track-color="#FFEEDA" stroke-width="4px"></van-progress>

            </view>
            <view>
            </view>
          </view>
          <view style="width: 50%;margin-left: 20rpx;margin-top: 10rpx">
            <view style="font-size: 25rpx;margin-right: 10rpx;">1-2</view>
            <view style="margin-left: 30px;margin-top: -20rpx;">
              <van-progress color="#FE8D19" percentage="15" track-color="#FFEEDA" stroke-width="4px"></van-progress>

            </view>
            <view>
            </view>
          </view>
        </view>
        <view class="rightParts">
          <view style="font-size: x-large;font-weight: bold;">9.0</view>
          <view style="margin-top: -50rpx;margin-left: 90rpx;">非常推荐</view>
          <view style="color: #9CA5B6;font-size: 25rpx;margin-top: 30rpx;">xxx人已入</view>
          <view style="color: #9CA5B6;font-size: 25rpx;margin-left: 130rpx;margin-top: -35rpx;">xxx人想要</view>
        </view>
      </view>
    </view>
    <scroll-view class="tab stickyClass" scroll-with-animation="true" scroll-into-view='{{indexMaodian}}'>
      <view bindtap='clickMenu' class="tab-item {{currentTab == 'modelPages' ? 'active' : ''}}" data-id="modelPages" id='modelPage'>模型图</view>
      <view bindtap='clickMenu' class="tab-item {{currentTab == 'messages' ? 'active' : ''}}" data-id="messages" id='message'>资料</view>
      <view bindtap='clickMenu' class="tab-item {{currentTab == 'evaluates' ? 'active' : ''}}" data-id="evaluates" id='evaluate'>评价</view>
    </scroll-view>
    <scroll-view scroll-into-view='{{storeDetail}}' style="height: 1200rpx;" scroll-y class="scroll-two" bindscroll="onPageScroll">
      <view class="advertisingMap" id="modelPages">
        <view class="advertisingMapText">官方宣传图</view>
        <scroll-view scroll-x>
          <view style="display: flex;">
            <van-image width="100" height="100" fit="cover" src="https://img.yzcdn.cn/vant/cat.jpeg" class="maps"></van-image>
            <van-image width="100" height="100" fit="cover" src="https://img.yzcdn.cn/vant/cat.jpeg" class="maps"></van-image>
            <van-image width="100" height="100" fit="cover" src="https://img.yzcdn.cn/vant/cat.jpeg" class="maps"></van-image>
            <van-image width="100" height="100" fit="cover" src="https://img.yzcdn.cn/vant/cat.jpeg" class="maps"></van-image>
            <van-image width="100" height="100" fit="cover" src="https://img.yzcdn.cn/vant/cat.jpeg" class="maps"></van-image>
          </view>
        </scroll-view>
      </view>
      <view class="information" id="messages">
        <view class="informationText">
          模型参数
        </view>
        <view class="informationTexts">
          <view style="width: 130rpx;  text-align-last: justify;">
            制作厂商
          </view>
          <view style="margin-left: 30rpx; line-height: 50rpx;font-size: 30rpx;">
            {{manufacturer}}
          </view>
        </view>
        <view class="informationTexts">
          <view style="width: 130rpx;  text-align-last: justify;">
            品牌系列
          </view>
          <view style="margin-left: 30rpx; line-height: 50rpx;font-size: 30rpx;">
            {{brand}}
          </view>
        </view>
        <view class="informationTexts">
          <view style="width: 130rpx;  text-align-last: justify;">
            产品比例
          </view>
          <view style="margin-left: 30rpx; line-height: 50rpx;font-size: 30rpx;">
            {{product}}
          </view>
        </view>
        <view class="informationTexts">
          <view style="width: 130rpx;  text-align-last: justify;">
            尺寸信息
          </view>
          <view style="margin-left: 30rpx; line-height: 50rpx;font-size: 30rpx;">
            {{size}}
          </view>
        </view>
        <view class="informationTexts">
          <view style="width: 130rpx;  text-align-last: justify;">
